<h2>Login</h2>

<%= form_tag "/users/auth/ldap/callback" do %>
  <div><%= label_tag :username %><br />
  <%= text_field_tag :username, nil, :autofocus=>true %></div>

  <div><%= label_tag :password %><br />
  <%= password_field_tag :password %></div>

  <div><%= submit_tag "Login" %></div>
<% end %>


