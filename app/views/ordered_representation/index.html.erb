<h1>Listing ordered_representations</h1>

<table>
  <tr>
    <th>Label</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ordered_representations.each do |ordered_representation| %>
  <tr>
    <td><%= ordered_representation.label %></td>
    <td><%= link_to 'Show', ordered_representation %></td>
    <td><%= link_to 'Edit', edit_ordered_representation_path(ordered_representation) %></td>
    <td><%= link_to 'Destroy', ordered_representation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

