<%= form_for @work, :html=>{:multipart=>true}, url: work_file_path do |f| %>
    <%= fields_for :work do |builder| %>
        <%= render 'metadata_fields', :f => builder %>
    <% end %>
    <br>
    <div class="action">
      <%= f.submit "Next" %>
      <%= f.submit 'Save and Finish' %>
    </div>
<% end %>

