<h1>Editing Ordered Instance Permission</h1>
<%= form_for @single_file_instance, :url => {:action=> "update_permission"}  do |f| %>
    <%= fields_for :@single_file_instance do |builder| %>
        <%= render 'permission', :f => builder %>
    <% end %>
    <br/>
    <%= f.submit 'Update Permission' %>
<% end %>
<%= render 'footer' %>
